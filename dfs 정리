def dfs(v):
    visited[v] = 1 # True
    print(v, end='')
    for i in graph[v]:
        if not visited[i]:
            dfs(i)

# graph는 인접리스트로 노드를 표현한것
graph = [    # 0번은 사용안함
    [],      # 인접행렬
    [2, 3, 8] # 01100001
    [1, 7,], # 10000010
    [1, 4, 5,], #10011000
    [3, 5], # 00101000
    [3, 4], # 00110000
    [7], # 00000010
    [2, 6, 8], # 01000101
    [1, 7], # 10000010
]
visited = [0] * 9 # [0, 0, 0, 0, 0, 0, 0, 0],
dfs(1) # 1번 노드부터 출발

# 1 2 3 4 5 6 7
